  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.16.2",
    "express-favicon": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.0"
  }
}
